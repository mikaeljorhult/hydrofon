{
    "name": "mikaeljorhult/hydrofon",
    "description": "Equipment booking system.",
    "keywords": [
        "hydrofon",
        "equipment",
        "booking",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0.2",
        "blade-ui-kit/blade-heroicons": "^2.0.4",
        "calebporzio/sushi": "^2.4",
        "coderello/laraflash": "^5.0",
        "eluceo/ical": "^2.7",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.34",
        "laravel/tinker": "^2.7.2",
        "laravel/ui": "^3.4.6",
        "laravolt/avatar": "^4.1.7",
        "livewire/livewire": "^2.10.7",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-activitylog": "^4.6",
        "spatie/laravel-model-states": "^2.4.2",
        "spatie/laravel-model-status": "^1.11",
        "spatie/laravel-query-builder": "^5.0.3",
        "staudenmeir/laravel-adjacency-list": "^1.11.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "brianium/paratest": "^6.6.4",
        "fakerphp/faker": "^1.20",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.1",
        "phpunit/phpunit": "^9.5.25",
        "spatie/laravel-ignition": "^1.5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "setup": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan migrate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
