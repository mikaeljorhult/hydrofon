{
    "name": "mikaeljorhult/hydrofon",
    "description": "Equipment booking system.",
    "keywords": [
        "hydrofon",
        "equipment",
        "booking",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-heroicons": "^2.1",
        "calebporzio/sushi": "^2.4",
        "eluceo/ical": "^2.7",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/fortify": "^1.17",
        "laravel/framework": "^10.3",
        "laravel/tinker": "^2.8",
        "laravolt/avatar": "^5.0",
        "livewire/livewire": "^3.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-model-states": "^2.4.2",
        "spatie/laravel-model-status": "^1.11",
        "spatie/laravel-query-builder": "^5.1",
        "staudenmeir/laravel-adjacency-list": "^1.13"
    },
    "require-dev": {
        "alebatistella/duskapiconf": "^1.2",
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "brianium/paratest": "^7.0",
        "fakerphp/faker": "^1.20",
        "laravel/dusk": "^7.10",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "protonemedia/laravel-dusk-fakes": "^1.4",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "setup": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan migrate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
